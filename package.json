{
  "name": "greenid-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "build-local": "tinacms build --local --skip-indexing --skip-cloud-checks --port 3000 --datalayer-port 9001 && next build",
    "start": "tinacms build && next start",
    "export": "npm run build && next export",
    "lint": "next lint",
    "dev:build": "next build",
    "tina": "npm run build-local && npm run dev"
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.1",
    "@docusaurus/preset-classic": "^3.6.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redocusaurus": "^2.2.0",
    "tinacms": "^2.4.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "@tinacms/cli": "^1.7.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
